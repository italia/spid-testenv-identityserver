<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<feature id="org.wso2.carbon.identity.sso.saml.server" label="Identity SAML Single Sign-on Server Feature" provider-name="%providerName" version="5.3.0">
  <description>This feature contains the core bundles required for Back-end SAML2.0 based Single
        Sign-on functionality</description>
  <copyright>%copyright</copyright>
  <license url="%licenseURL">%license</license>
  <plugin id="opensaml" unpack="false" version="2.6.4.wso2v3"/>
  <plugin id="joda-time" unpack="false" version="2.8.2.wso2v1"/>
  <plugin id="org.wso2.carbon.identity.sso.saml" unpack="false" version="5.3.0"/>
  <require>
    <import feature="org.wso2.carbon.identity.application.authentication.framework.server" match="compatible" version="5.7.0"/>
    <import feature="org.wso2.carbon.security.mgt.server" match="compatible" version="5.7.0"/>
    <import feature="org.wso2.carbon.identity.application.mgt.server" match="compatible" version="5.7.0"/>
    <import feature="org.wso2.carbon.identity.core" match="compatible" version="5.7.0"/>
    <import feature="org.wso2.carbon.core" match="compatible" version="4.4.7"/>
    <import feature="org.wso2.carbon.idp.mgt.server" match="compatible" version="5.7.0"/>
  </require>
</feature>
